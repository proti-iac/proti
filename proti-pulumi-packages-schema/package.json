{
  "name": "@proti-iac/pulumi-packages-schema",
  "version": "0.0.0",
  "description": "Automated Testing for IaC Programs",
  "author": "Daniel Sokolowski <github@d.sokolowski.org>",
  "license": "Apache-2.0",
  "exports": {
    ".": "./bin/index.js",
    "./generator-plugin": "./bin/generator-plugin.js",
    "./oracle-plugin": "./bin/oracle-plugin.js",
    "./pulumi-package-schema": "./bin/pulumi-package-metaschema.js"
  },
  "types": "./bin/index.d.ts",
  "files": [
    "bin"
  ],
  "scripts": {
    "clean": "yarn g:clean",
    "build": "yarn g:build:tspc",
    "dev": "yarn g:build:tspc && yarn jest -c \"{ \\\"preset\\\": \\\"../proti-test-runner/jest-preset.js\\\", \\\"globals\\\": { \\\"proti\\\": { \\\"testCoordinator\\\": { \\\"generator\\\": \\\"$(pwd)/bin/generator-plugin\\\", \\\"oracles\\\": [ \\\"$(pwd)/bin/oracle-plugin\\\" ] }, \\\"plugins\\\": { \\\"pulumi-packages-schema\\\": {} } } } }\" --roots",
    "lint": "yarn g:lint",
    "test": "yarn g:test",
    "prepack": "yarn g:prepack && yarn g:license:copy && cp README.md READMEbu.md && yarn g:readme:urls",
    "postpack": "rm README.md LICENSE && mv READMEbu.md README.md",
    "update-schema": "curl https://raw.githubusercontent.com/pulumi/pulumi/master/pkg/codegen/schema/pulumi.json | yarn json2ts > src/pulumi-generated.d.ts",
    "postinstall": "ts-patch install"
  },
  "dependencies": {
    "@proti-iac/core": "workspace:^",
    "@pulumi/pulumi": "^3.91.1",
    "fast-check": "^3.13.2",
    "ts-patch": "3.0.2",
    "typia": "^5.2.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@types/node": "^20.12.11",
    "jest": "^29.7.0",
    "pure-rand": "^6.0.4",
    "typescript": "^5.4.5"
  }
}
