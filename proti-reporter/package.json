  },
  "devDependencies": {
    "@proti-iac/core": "workspace:^",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.12.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5"
  }
}
