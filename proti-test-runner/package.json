{
  "name": "@proti-iac/test-runner",
  "version": "0.0.0",
  "description": "Automated Testing for IaC Programs",
  "author": "Daniel Sokolowski <github@d.sokolowski.org>",
  "license": "Apache-2.0",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "files": [
    "bin",
    "jest-preset.js"
  ],
  "scripts": {
    "clean": "yarn g:clean",
    "build": "yarn g:build",
    "dev": "yarn g:build && yarn jest -c jest-preset.js --roots",
    "lint": "yarn g:lint",
    "test": "yarn g:test",
    "prepack": "yarn g:prepack && cp ../README.md . && cp ../LICENSE .",
    "postpack": "rm README.md LICENSE"
  },
  "dependencies": {
    "@jest/expect": "^29.7.0",
    "@proti-iac/core": "workspace:^",
    "@pulumi/pulumi": "^3.91.1",
    "fast-check": "^3.13.2",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@jest/environment": "^29.7.0",
    "@jest/test-result": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@proti-iac/runner": "workspace:^",
    "@types/jest": "^29.5.7",
    "@types/node": "^18.18.8",
    "jest-haste-map": "^29.7.0",
    "jest-resolve": "^29.7.0",
    "jest-runtime": "^29.7.0",
    "typescript": "^5.2.2"
  }
}
