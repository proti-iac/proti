{
  "name": "@proti-iac/plugins-demo",
  "version": "0.0.0",
  "description": "Automated Testing for IaC Programs",
  "author": "Daniel Sokolowski <github@d.sokolowski.org>",
  "license": "Apache-2.0",
  "main": "./bin/index.js",
  "exports": {
    ".": "./bin/index.js",
    "./demo-generator-plugin": "./bin/demo-generator-plugin.js",
    "./demo-oracle-plugin": "./bin/demo-oracle-plugin.js"
  },
  "types": "./bin/index.d.ts",
  "files": [
    "bin"
  ],
  "scripts": {
    "clean": "yarn g:clean",
    "build": "yarn g:build:tspc",
    "dev": "yarn g:build:tspc && yarn jest -c \"{ \\\"preset\\\": \\\"../proti-test-runner/jest-preset.js\\\", \\\"globals\\\": { \\\"proti\\\": { \\\"testCoordinator\\\": { \\\"generator\\\": \\\"$(pwd)/bin/demo-generator-plugin\\\", \\\"oracles\\\": [ \\\"$(pwd)/bin/demo-oracle-plugin\\\" ] }, \\\"plugins\\\": { \\\"plugins-demo\\\": {} } } } }\" --roots",
    "lint": "yarn g:lint",
    "test": "yarn g:test",
    "prepack": "yarn g:prepack && yarn g:license:copy && cp README.md READMEbu.md && yarn g:readme:urls",
    "postpack": "rm README.md LICENSE && mv READMEbu.md README.md",
    "postinstall": "ts-patch install"
  },
  "dependencies": {
    "@proti-iac/core": "workspace:^",
    "@pulumi/pulumi": "^3.91.1",
    "fast-check": "^3.13.2",
    "jest-runtime": "^29.7.0",
    "ts-patch": "3.0.2",
    "typia": "^5.2.4"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.7",
    "@types/node": "^18.18.8",
    "jest": "^29.7.0",
    "typescript": "^5.2.2"
  }
}
