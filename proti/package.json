{
  "name": "proti-monorepo",
  "private": true,
  "packageManager": "yarn@3.3.1",
  "workspaces": [
    "proti-cli",
    "proti-core",
    "proti-runner",
    "proti-test-runner"
  ],
  "scripts": {
    "g:all": "yarn workspaces foreach -pti --exclude proti-monorepo run $0",
    "g:pkgroot": "bash -c 'if [ -e \"$0/$1\" ] && [ $0 != $PROJECT_CWD ]; then cd $0 && eval $3; else echo \"\\\"$2\\\" must be executed in a package root with a $1, not in $0\"; fi'",
    "g:clean": "yarn g:pkgroot $INIT_CWD package.json clean 'rm -rf bin'",
    "clean": "yarn g:all clean",
    "g:build": "yarn g:pkgroot $INIT_CWD tsconfig.json build tsc",
    "build": "yarn g:all build",
    "g:lint": "eslint --ext .ts --ignore-pattern bin/ $INIT_CWD",
    "lint": "yarn g:lint",
    "g:test": "yarn g:pkgroot $INIT_CWD tsconfig.json test 'yarn build && INIT_CWD=$(pwd) && cd $PROJECT_CWD && jest --roots $INIT_CWD'",
    "test": "yarn g:all test",
    "g:prepack": "yarn g:pkgroot $INIT_CWD package.json prepack 'yarn clean && yarn build'",
    "prepack": "yarn g:all prepack"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "resolutions": {
    "jest-cli@^29.3.1": "patch:jest-cli@npm%3A29.3.1#./.yarn/patches/jest-cli-npm-29.3.1-4b4c5a01f0.patch",
    "jest-cli@^29.4.0": "patch:jest-cli@npm%3A29.4.0#./.yarn/patches/jest-cli-npm-29.4.0-38daae8866.patch",
    "jest-runtime@^29.4.0": "patch:jest-runtime@npm%3A29.4.0#./.yarn/patches/jest-runtime-npm-29.4.0-81b97db3a5.patch",
    "@types/jest@^29.4.0": "patch:@types/jest@npm%3A29.4.0#./.yarn/patches/@types-jest-npm-29.4.0-e28b79cadc.patch"
  }
}
