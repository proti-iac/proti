{
  "name": "proti-monorepo",
  "private": true,
  "packageManager": "yarn@3.3.1",
  "workspaces": [
    "proti-cli",
    "proti-core",
    "proti-demo",
    "proti-runner",
    "proti-test-runner",
    "proti-transformer",
    "proti-reporter"
  ],
  "scripts": {
    "g:all": "yarn workspaces foreach $1 --exclude proti-monorepo run $0",
    "g:pkgroot": "bash -c 'if [ -e \"$0/$1\" ] && [ $0 != $PROJECT_CWD ]; then cd $0 && eval $3; else echo \"\\\"$2\\\" must be executed in a package root with a $1, not in $0\"; fi'",
    "g:clean": "yarn g:pkgroot $INIT_CWD package.json clean 'rm -rf bin'",
    "clean": "yarn g:all clean -vip",
    "g:build": "yarn g:pkgroot $INIT_CWD tsconfig.json build 'yarn tsc'",
    "g:build:ttsc": "yarn g:pkgroot $INIT_CWD tsconfig.json build 'yarn ttsc'",
    "build": "yarn g:all build '-vip --topological-dev'",
    "g:lint": "eslint --ext .ts --ignore-pattern bin/ $INIT_CWD",
    "lint": "yarn g:lint",
    "g:test": "yarn g:pkgroot $INIT_CWD tsconfig.json test 'yarn build && INIT_CWD=$(pwd) && cd $PROJECT_CWD && jest --roots $INIT_CWD'",
    "test": "yarn build && yarn jest",
    "g:prepack": "yarn g:pkgroot $INIT_CWD package.json prepack 'yarn clean && yarn build'",
    "prepack": "yarn g:all prepack -vitp"
  },
  "devDependencies": {
    "@proti/transformer": "workspace:^",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tst-reflect-transformer": "alpha",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typia": "^3.7.0"
  },
  "resolutions": {
    "@types/jest@^29.5.0": "patch:@types/jest@npm%3A29.5.0#./.yarn/patches/@types-jest-npm-29.5.0-18eef6c3cc.patch",
    "jest-cli@^29.5.0": "patch:jest-cli@npm%3A29.5.0#./.yarn/patches/jest-cli-npm-29.5.0-3746aee7e9.patch",
    "jest-runtime@^29.5.0": "patch:jest-runtime@npm%3A29.5.0#./.yarn/patches/jest-runtime-npm-29.5.0-e692162e2b.patch"
  }
}
