{
  "name": "proti",
  "version": "0.0.0",
  "description": "Property-based testing of Pulumi TypeScript infrastructure as code programs",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "files": [
    "bin",
    "jest-proti.config.js",
    "patches"
  ],
  "bin": {
    "proti": "./bin/run.js"
  },
  "repository": "https://github.com/prg-grp/iac-testing",
  "author": "Daniel Sokolowski",
  "license": "MIT",
  "private": false,
  "scripts": {
